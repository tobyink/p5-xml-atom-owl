          </dcs:Addition>
        </dcs:item>
        <dcs:item>
          <dcs:Addition>
            <rdfs:label xml:lang="en">Support for Feed Paging and Archiving</rdfs:label>
            <rdfs:seeAlso rdf:resource="http://www.ietf.org/rfc/rfc5005.txt"/>
          </dcs:Addition>
        </dcs:item>
        <dcs:item>
          <dcs:Addition>
            <rdfs:label xml:lang="en">Support for Atom License Extension</rdfs:label>
            <rdfs:seeAlso rdf:resource="http://www.ietf.org/rfc/rfc4946.txt"/>
          </dcs:Addition>
        </dcs:item>
        <dcs:item>
          <dcs:Addition>
            <rdfs:label xml:lang="en">Support arbitrary other extensions using XML literals.</rdfs:label>
          </dcs:Addition>
        </dcs:item>
